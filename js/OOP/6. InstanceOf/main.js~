/* obj instanceof Class */

class Rabbit {}

let rabbit = new Rabbit();
console.log(rabbit instanceof Rabbit); // returns true;

// it also works with functions

function foo() {}

let Foo = new foo();
foo.prototype = { };

console.log("Changed Prototype : " + Foo instanceof foo);
console.log(new foo() instanceof foo); // returns true;

let arr = [1, 2, 3];

console.log(arr instanceof Array); // returns true;
//Please note that arr also belongs to the Object class.Thatâ€™s because Array prototypically inherits from Object.
console.log(arr instanceof Object); // returns true;

class Animal {
	static [Symbol.hasInstance](obj) {
		if (obj.canEat) return true;
	}
}

let obj = { canEat: true };

console.log(obj instanceof Animal); // returns true;


